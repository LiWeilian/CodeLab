<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup> 
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
  <appSettings>
    <add key="dataservers" value="[{'SN':'001','IP':'172.16.1.2','PORT':'6008'},{'SN':'002','IP':'172.16.1.2','PORT':'6009'}]"/>
  </appSettings>
	<system.serviceModel>
		<services>
			<service name="GDDST.DI.DataServiceWCF.DataService" 
                 behaviorConfiguration="DataServiceBehavior">
				<endpoint address="" 
                    binding="webHttpBinding" 
                    contract="GDDST.DI.DataServiceWCF.IDataService"
                    behaviorConfiguration="web">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex" 
                    binding="mexHttpBinding" 
                    contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8733/GDDST.DI.DataServiceWCF/DataService/" />
					</baseAddresses>
				</host>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="DataServiceBehavior">
					<serviceMetadata httpGetEnabled="true" />
					<serviceDebug includeExceptionDetailInFaults="true" />
				</behavior>
				<behavior>
					<serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
					<serviceDebug includeExceptionDetailInFaults="False" />
				</behavior>
			</serviceBehaviors>
			<endpointBehaviors>
				<behavior name="web">
					<webHttp/>
				</behavior>
			</endpointBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>