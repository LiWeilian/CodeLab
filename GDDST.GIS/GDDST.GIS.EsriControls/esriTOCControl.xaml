<UserControl x:Class="GDDST.GIS.EsriControls.esriTOCControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GDDST.GIS.EsriControls"
             xmlns:esri="clr-namespace:ESRI.ArcGIS.Controls;assembly=ESRI.ArcGIS.AxControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="tocCtrlGrid" Margin="0,0,0,0">

        <WindowsFormsHost x:Name="tocHost" HorizontalAlignment="Stretch" Margin="5,5,5,5" VerticalAlignment="Stretch">
            <esri:AxTOCControl x:Name="tocCtrl" Dock="Fill"/>
        </WindowsFormsHost>
    </Grid>
</UserControl>
